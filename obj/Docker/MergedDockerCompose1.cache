services:
  db:
    container_name: pg_container
    environment:
      POSTGRES_DB: test_db
      POSTGRES_PASSWORD: root
      POSTGRES_USER: root
    image: postgres
    ports:
    - published: 5432
      target: 5432
    restart: always
  employeewebservice:
    build:
      context: C:\Users\Mikhail\source\repos\EmployeeWebService
      dockerfile: EmployeeWebService/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: EmployeeWebService
      target: base
    container_name: EmployeeWebService
    depends_on:
      db:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: employeewebservice:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/EmployeeWebService.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\Mikhail\source\repos\EmployeeWebService\EmployeeWebService:/app:rw
    - C:\Users\Mikhail\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Mikhail\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Mikhail\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Mikhail\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Mikhail\source\repos\EmployeeWebService:/src:rw
version: '3.4'